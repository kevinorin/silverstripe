{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.2",
        "silverstripe/cms": "3.7.3",
        "silverstripe/framework": "3.7.4",
        "silverstripe/siteconfig": "3.7.3",
        "silverstripe/reports": "3.7.3",
        "undefinedoffset/sortablegridfield": "1.0.2",
        "silverstripe/userforms": "4.6.0",
        "symbiote/silverstripe-gridfieldextensions": "2.0.2",
        "silverstripe/segment-field": "1.0.3",
        "silverstripe/translatable": "^2.3"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~3.7@stable"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
